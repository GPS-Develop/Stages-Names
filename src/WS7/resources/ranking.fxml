<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="8.0" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity" vgap="8.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WS7.controllers.RankingController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Enter the Year:" GridPane.halignment="LEFT">
         <padding>
            <Insets bottom="8.0" right="8.0" top="8.0" />
         </padding>
      </Label>
      <Label text="Enter the Gender:" GridPane.halignment="LEFT" GridPane.rowIndex="1">
         <padding>
            <Insets bottom="8.0" right="8.0" top="8.0" />
         </padding>
      </Label>
      <Label text="Enter the Name:" GridPane.halignment="LEFT" GridPane.rowIndex="2">
         <padding>
            <Insets bottom="8.0" right="8.0" top="8.0" />
         </padding>
      </Label>
      <Button fx:id="submitQueryButton" mnemonicParsing="false" onAction="#submitQueryClicked" prefWidth="100.0" text="Submit Query" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
      <Button fx:id="MainMenuButton" mnemonicParsing="false" onAction="#MainMenuClicked" prefWidth="100.0" text="MainMenu" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
      <TextField fx:id="yearTextField" GridPane.columnIndex="1" />
      <TextField fx:id="genderTextField" maxWidth="-Infinity" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="nameTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</GridPane>
