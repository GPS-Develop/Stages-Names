<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="125.0" minWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="WS7.controllers.SameNamesController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Baby Ranking Year:" GridPane.halignment="RIGHT">
         <GridPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </GridPane.margin>
      </Text>
      <TextField fx:id="sameNameTextField" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </GridPane.margin>
      </TextField>
      <Button fx:id="searchButton" minWidth="100.0" mnemonicParsing="false" onAction="#searchButtonClicked" text="Search" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP">
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
         <GridPane.margin>
            <Insets bottom="8.0" left="4.0" right="8.0" top="8.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="mainMenuButton" minWidth="100.0" mnemonicParsing="false" onAction="#mainMenuClicked" text="MainMenu" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP">
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
         <GridPane.margin>
            <Insets bottom="8.0" left="4.0" right="8.0" top="8.0" />
         </GridPane.margin>
      </Button>
   </children>
</GridPane>
